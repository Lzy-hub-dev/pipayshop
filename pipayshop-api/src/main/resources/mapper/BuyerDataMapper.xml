<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.pipayshopapi.mapper.BuyerDataMapper">


    <select id="selectBuyerDataById" resultType="com.example.pipayshopapi.entity.vo.BuyerDataVO">
        select user_name,address,phone
        FROM buyer_data
        WHERE id=#{id};
    </select>
</mapper>
